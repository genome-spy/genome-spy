        "d3-array": "^2.3.3",
        "d3-color": "^1.4.0",
        "d3-format": "^1.4.1",
        "d3-scale": "^3.2.0",
        "eventemitter3": "^3.1.2",
        "fromentries": "^1.2.0",
        "mapsort": "^1.0.2",
