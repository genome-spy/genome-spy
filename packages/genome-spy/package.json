{
  "name": "genome-spy",
  "description": "GenomeSpy, a genome browser with WebGL and a grammar of graphics",
  "author": {
    "name": "Kari Lavikka",
    "email": "kari.lavikka@helsinki.fi"
  },
  "contributors": [],
  "license": "BSD-2-Clause",
  "version": "0.8.0",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "github:tuner/genome-spy",
    "directory": "packages/genome-spy"
  },
  "sideEffects": [
    "*.scss",
    "**/*-wc.js"
  ],
  "scripts": {
    "dev": "node dev-server.js",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "checkSpec": "tsc --allowJs --checkJs --strict --noEmit --moduleResolution node --target es6 src/spec/root.d.ts",
    "buildSchema": "ts-json-schema-generator --path 'src/spec/*.ts' --type ViewSpec > dist/genome-spy-schema.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@types/d3-array": "^2.9.0",
    "@types/d3-dsv": "^2.0.1",
    "@types/d3-ease": "^2.0.0",
    "@types/d3-format": "^2.0.0",
    "@types/d3-interpolate": "^2.0.0",
    "@types/d3-scale": "^3.2.2",
    "d3-array": "^2.11.0",
    "d3-color": "^2.0.0",
    "d3-dsv": "^2.0.0",
    "d3-ease": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-scale": "^3.2.3",
    "heapify": "^0.3.0",
    "idb": "^6.0.0",
    "immer": "^9.0.5",
    "internmap": "^1.0.0",
    "lit": "^2.0.2",
    "lz-string": "^1.4.4",
    "mapsort": "^1.0.3",
    "snarkdown": "^2.0.0",
    "twgl.js": "^4.19.1",
    "vega-expression": "^2.6.5",
    "vega-loader": "^4.4.0",
    "vega-scale": "^7.1.1",
    "vega-util": "^1.16.0"
  }
}
