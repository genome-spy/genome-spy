{
  "name": "genome-spy",
  "description": "GenomeSpy, a genome browser with WebGL and a grammar of graphics",
  "author": {
    "name": "Kari Lavikka",
    "email": "kari.lavikka@helsinki.fi"
  },
  "contributors": [],
  "license": "BSD-2-Clause",
  "version": "0.0.4",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tuner/genome-spy"
  },
  "sideEffects": [
    "*.scss"
  ],
  "scripts": {
    "start": "webpack-dev-server --progress --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "buildSinglePage": "webpack --config webpack.singlepage.prod.js",
    "buildSchema": "ts-json-schema-generator -p src/spec/transform.ts > dist/genome-spy-schema.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@types/d3-array": "^2.0.0",
    "@types/d3-dsv": "^1.0.36",
    "@types/d3-format": "^1.3.1",
    "@types/d3-interpolate": "^1.3.1",
    "@types/d3-scale": "^2.2.0",
    "array-flat-polyfill": "^1.0.1",
    "d3-array": "^2.5.0",
    "d3-color": "^1.4.1",
    "d3-dsv": "^1.2.0",
    "d3-format": "^1.4.4",
    "d3-scale": "^3.2.1",
    "eventemitter3": "^4.0.4",
    "fromentries": "^1.2.1",
    "heapify": "^0.3.0",
    "lit-html": "^1.2.1",
    "mapsort": "^1.0.3",
    "math.gl": "^2.3.3",
    "tinyqueue": "^2.0.3",
    "twgl.js": "^4.15.2",
    "vega-expression": "^2.6.5",
    "vega-loader": "^4.3.0",
    "vega-scale": "^7.0.0",
    "vega-util": "^1.14.1"
  }
}
