{
  "name": "genome-spy",
  "description": "GenomeSpy, a genome browser with WebGL and a grammar of graphics",
  "author": {
    "name": "Kari Lavikka",
    "email": "kari.lavikka@helsinki.fi"
  },
  "contributors": [],
  "license": "BSD-2-Clause",
  "version": "0.0.5",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tuner/genome-spy"
  },
  "sideEffects": [
    "*.scss"
  ],
  "scripts": {
    "start": "webpack-dev-server --progress --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "buildSinglePage": "webpack --config webpack.singlepage.prod.js",
    "buildSchema": "ts-json-schema-generator -p src/spec/transform.ts > dist/genome-spy-schema.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@types/d3-array": "^2.9.0",
    "@types/d3-dsv": "^2.0.1",
    "@types/d3-ease": "^2.0.0",
    "@types/d3-format": "^2.0.0",
    "@types/d3-interpolate": "^2.0.0",
    "@types/d3-scale": "^3.2.2",
    "d3-array": "^2.11.0",
    "d3-color": "^2.0.0",
    "d3-dsv": "^2.0.0",
    "d3-ease": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-scale": "^3.2.3",
    "heapify": "^0.3.0",
    "idb": "^6.0.0",
    "immer": "^7.0.14",
    "internmap": "^1.0.0",
    "lit-html": "^1.3.0",
    "lz-string": "^1.4.4",
    "mapsort": "^1.0.3",
    "snarkdown": "^2.0.0",
    "twgl.js": "^4.19.1",
    "vega-expression": "^2.6.5",
    "vega-loader": "^4.4.0",
    "vega-scale": "^7.1.1",
    "vega-util": "^1.16.0"
  }
}
