{
    "name": "genome-spy",
    "description": "GenomeSpy, a genome browser with WebGL and a grammar of graphics",
    "author": {
        "name": "Kari Lavikka",
        "email": "kari.lavikka@helsinki.fi"
    },
    "contributors": [],
    "license": "BSD-2-Clause",
    "version": "0.0.1",
    "main": "dist/index.js",
    "module": "src/index.js",
