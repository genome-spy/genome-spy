{
  "name": "genome-spy",
  "description": "GenomeSpy, a genome browser with WebGL and a grammar of graphics",
  "author": {
    "name": "Kari Lavikka",
    "email": "kari.lavikka@helsinki.fi"
  },
  "contributors": [],
  "license": "BSD-2-Clause",
  "version": "0.13.0",
  "main": "dist/index.js",
  "module": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./*": "./src/*"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "github:tuner/genome-spy",
    "directory": "packages/genome-spy"
  },
  "scripts": {
    "dev": "node dev-server.js",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "checkSpec": "tsc --allowJs --checkJs --strict --noEmit --moduleResolution node --target es6 src/spec/root.d.ts",
    "buildSchema": "ts-json-schema-generator --path 'src/spec/*.ts' --type ViewSpec > dist/genome-spy-schema.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/d3-array": "^3.0.2",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-ease": "^3.0.0",
    "@types/d3-format": "^3.0.1",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-scale": "^4.0.2",
    "d3-array": "^3.1.1",
    "d3-color": "^3.0.1",
    "d3-ease": "^3.0.1",
    "d3-format": "^3.0.1",
    "flatqueue": "^1.2.1",
    "idb": "^6.1.5",
    "internmap": "^2.0.3",
    "lit": "^2.0.2",
    "lz-string": "^1.4.4",
    "mapsort": "^1.0.3",
    "redux-batched-actions": "^0.5.0",
    "redux-undo": "^1.0.1",
    "snarkdown": "^2.0.0",
    "twgl.js": "^4.19.1",
    "vega-expression": "^5.0.0",
    "vega-loader": "^4.4.0",
    "vega-scale": "^7.1.1",
    "vega-util": "^1.16.0"
  }
}
