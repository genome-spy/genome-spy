{
  "$schema": "https://unpkg.com/@genome-spy/core/dist/schema.json",

  "data": {
    "sequence": {
      "start": 1,
      "stop": 100,
      "step": 5
    }
  },
  "mark": "point",
  "encoding": {
    "x": {
      "field": "data",
      "type": "quantitative"
    },
    "y": {
      "field": "data",
      "type": "quantitative",
      "scale": {
        "type": "log",
        "domain": [1, 10000]
      }
    }
  }
}
